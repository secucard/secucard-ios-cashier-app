use_frameworks!

target 'CashierApp', :exclusive => true do
  
  pod 'SecucardConnectSDK', '~> 0.2'
  
  pod 'SwiftyJSON'
  pod 'SnapKit'
  pod 'Alamofire'
  pod 'TFBarcodeScanner'
  pod 'HockeySDK'
  
end

target 'CashierAppTests', :exclusive => true do
  
  pod 'SecucardConnectSDK', '~> 0.2'
  
  pod 'Quick'
  pod 'Nimble', '= 2.0.0-rc.3'
  pod 'FBSnapshotTestCase'
  pod 'Nimble-Snapshots'
  
end
